{
  "name": "openvpn-config-splitter",
  "version": "0.1.1",
  "description": "Splits OpenVPN (.ovpn) files into separate files for private key, user+ca certificates and tls-auth key, for use with network-manager in debian/ubuntu",
  "author": {
    "name": "Espen Hovlandsdal",
    "email": "espen@hovlandsdal.com",
    "url": "http://rexxars.com"
  },
  "license": "MIT",
  "preferGlobal": true,
  "bin": {
    "ovpnsplit": "lib/cli.js"
  },
  "main": "lib/splitter.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/rexxars/openvpn-config-splitter.git"
  },
  "scripts": {
    "test": "./node_modules/.bin/tape test/*.test.js"
  },
  "keywords": [
    "openvpn",
    "split",
    "config",
    "vpn",
    "ubuntu",
    "network",
    "manager"
  ],
  "bugs": {
    "url": "https://github.com/rexxars/openvpn-config-splitter/issues"
  },
  "dependencies": {
    "argparse": "~0.1.15",
    "errno": "~0.1.0",
    "async": "~0.2.10"
  },
  "devDependencies": {
    "tape": "~2.5.0",
    "mock-fs": "~2.2.0"
  }
}
